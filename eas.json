{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_ROUTER_APP_ROOT": "app",
        "EXPO_PUBLIC_REVENUECAT_GOOGLE_KEY": "$REVENUECAT_GOOGLE_KEY",
        "EXPO_PUBLIC_VERCEL_API_URL": "https://ailifecopilot.vercel.app"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_ROUTER_APP_ROOT": "app"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_ROUTER_APP_ROOT": "app",
        "EXPO_PUBLIC_REVENUECAT_GOOGLE_KEY": "$REVENUECAT_GOOGLE_KEY",
        "EXPO_PUBLIC_VERCEL_API_URL": "https://ailifecopilot.vercel.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
